			<bind nodeset="/data/Decision_Variables/GRP-dangerSigns" />
			<bind nodeset="/data/Decision_Variables/GRP-dangerSigns/VAR-dangerSignMUAC" relevant="selected(/data/GRP-dangerSigns/LMD-CHK-dangerSigns, 'LMD-OPT-redMUAC')" calculate="&quot; &#10;    - Malnutrition (Red MUAC)&quot;" />
			<bind nodeset="/data/Decision_Variables/GRP-dangerSigns/VAR-feetSwelling" relevant="selected(/data/GRP-dangerSigns/LMD-CHK-dangerSigns, 'LMD-OPT-feetSwelling')" calculate="&quot; &#10;    - Feet Swelling&quot;" />
			<bind nodeset="/data/Decision_Variables/GRP-dangerSigns/VAR-dangerSignUnconscious" relevant="selected(/data/GRP-dangerSigns/LMD-CHK-dangerSigns, 'LMD-OPT-unconscious')" calculate="&quot; &#10;    - Unconscious or very sleepy&quot;" />
			<bind nodeset="/data/Decision_Variables/GRP-dangerSigns/VAR-dangerSignConvulsions" relevant="selected(/data/GRP-dangerSigns/LMD-CHK-dangerSigns, 'LMD-OPT-convulsions')" calculate="&quot; &#10;    - Convulsions (Jerking)&quot;" />
			<bind nodeset="/data/Decision_Variables/GRP-dangerSigns/VAR-dangerSignNotEatingOrDrinking" relevant="selected(/data/GRP-dangerSigns/LMD-CHK-dangerSigns, 'LMD-OPT-problemsEatingAndDrinking')" calculate="&quot; &#10;    - Not eating or drinking anything&quot;" />
			<bind nodeset="/data/Decision_Variables/GRP-dangerSigns/VAR-dangerSignVomitingEverything" relevant="selected(/data/GRP-dangerSigns/LMD-CHK-dangerSigns, 'LMD-OPT-vomitingEverything')" calculate="&quot; &#10;    - Vomiting everything&quot;" />
			<bind nodeset="/data/Decision_Variables/GRP-dangerSigns/VAR-dangerSignBloodInStool" relevant="selected(/data/GRP-dangerSigns/LMD-CHK-dangerSigns, 'LMD-OPT-bloodInStool')" calculate="&quot; &#10;    - Blood in stool&quot;" />
			<bind nodeset="/data/Decision_Variables/GRP-dangerSigns/VAR-dangerSignCoughingMoreThan21Days" relevant="selected(/data/GRP-dangerSigns/LMD-CHK-dangerSigns, 'LMD-OPT-coughingMoreThan21Days')" calculate="&quot; &#10;    - Coughing for more than 21 days&quot;" />

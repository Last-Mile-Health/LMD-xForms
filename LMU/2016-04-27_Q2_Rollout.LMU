			<bind nodeset="/data/Decision_Variables/VAR-NotifySupervisor" relevant="(/data/GRP-homeTreatmentSigns/GRP-Diarrhea/LMD-VAL-diarrhea = '1' or /data/GRP-homeTreatmentSigns/GRP-Fever/LMD-VAL-fever = '1' or /data/GRP-homeTreatmentSigns/GRP-coughing/LMD-VAL-childCoughing = '1') and /data/GRP-reviewAssessment/LMD-VAL-visitType = 'followUpVisit3'" calculate="1" />
			<bind nodeset="/data/Decision_Variables/VAR-incorrectAge" relevant="/data/GRP-reviewAssessment/GRP-childAge/LMD-VAL-childAgeYears &gt; 5 or /data/GRP-reviewAssessment/GRP-childAge/LMD-VAL-childAgeYears = 5 and /data/GRP-reviewAssessment/GRP-childAge/LMD-VAL-ChildAgeMonths &gt; 1 or /data/GRP-reviewAssessment/GRP-childAge/LMD-VAL-childAgeYears &lt;= 0 and /data/GRP-reviewAssessment/GRP-childAge/LMD-VAL-ChildAgeMonths &lt; 2" calculate="1" />

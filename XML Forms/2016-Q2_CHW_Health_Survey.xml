<?xml version="1.0" encoding="UTF-8" ?>
<h:html xmlns:h="http://www.w3.org/1999/xhtml" xmlns:orx="http://openrosa.org/jr/xforms" xmlns="http://www.w3.org/2002/xforms" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:jr="http://openrosa.org/javarosa" xmlns:vellum="http://commcarehq.org/xforms/vellum">
	<h:head>
		<h:title>CHW: Health Survey</h:title>
		<model>
			<instance>
				<data xmlns:jrm="http://dev.commcarehq.org/jr/xforms" xmlns="http://openrosa.org/formdesigner/08B850EF-F8AB-47F6-B777-FAC1B22C538B" uiVersion="6" version="6" name="CHW: Health Survey">
					<LMD-DATABASE />
					<LMD-TABLE />
					<LMD-VAL-meta_UUID />
					<LMD-VAL-linkUUID />
					<LMD-VAL-meta_autoDate />
					<LMD-TIM-meta_dataEntry_startTime />
					<LMD-TIM-meta_dataEntry_endTime />
					<LMD-VAL-meta_dataSource />
					<LMD-VAL-meta_formVersion />
					<LMD-VAL-meta_deviceID />
					<LABEL-womanAge />
					<LMD-VAL-manualDate />
					<LMD-VAL-chwID />
					<LMD-VAL-communityID />
					<LMD-VAL-memberName />
					<LMD-VAL-householdID />
					<GRP-ancVisitation>
						<LMD-VAL-birthInFiveYears />
						<LMD-VAL-dateOfLastBirth />
						<LMD-VAL-clinicVisits />
						<LMD-VAL-townVisits />
					</GRP-ancVisitation>
					<LMD-VAL-hasChildren />
					<LMD-VAL-numberOfChildren />
					<LABEL-askQuestions />
					<LMD-RPT-staging_odk_children jr:template="">
						<LMD-PULL />
						<LMD-VAL-meta_UUID />
						<LMD-VAL-childName />
						<LMD-VAL-dateOfBirth />
						<LMD-VAL-bornInClinic />
						<LMD-VAL-stillAlive />
						<GRP-ageAtDeath>
							<LMD-VAL-deathAgeYears />
							<LMD-VAL-deathAgeMonths />
							<LMD-VAL-deathAgeDays />
						</GRP-ageAtDeath>
					</LMD-RPT-staging_odk_children>
					<LMD-VAL-numberOfVaccineCards />
					<LMD-RPT-staging_odk_vaccine jr:template="">
						<LMD-PULL />
						<LMD-VAL-meta_UUID />
						<LABEL-vaccineInfo />
						<LMD-VAL-childFirstName />
						<LMD-CHK-vaccineReceived />
						<LMD-VAL-BCG />
						<LMD-VAL-OPV0 />
						<LMD-VAL-OPV1 />
						<LMD-VAL-OPV2 />
						<LMD-VAL-OPV3 />
						<LMD-VAL-Penta1 />
						<LMD-VAL-Penta2 />
						<LMD-VAL-Penta3 />
						<LMD-VAL-Rota1 />
						<LMD-VAL-Rota2 />
						<LMD-VAL-Rota3 />
						<LMD-VAL-Pneumo1 />
						<LMD-VAL-Pneumo2 />
						<LMD-VAL-Pneumo3 />
						<LMD-VAL-IPV1 />
						<LMD-VAL-IPV2 />
						<LMD-VAL-IPV3 />
						<LMD-VAL-measles />
						<LMD-VAL-yellowFever />
					</LMD-RPT-staging_odk_vaccine>
				</data>
			</instance>
			<bind nodeset="/data/LMD-DATABASE" calculate="&quot;lastmile_chwdb&quot;" />
			<bind nodeset="/data/LMD-TABLE" calculate="&quot;staging_odk_healthsurvey&quot;" />
			<bind nodeset="/data/LMD-VAL-meta_UUID" calculate="uuid()" />
			<bind nodeset="/data/LMD-VAL-linkUUID" calculate="uuid()" />
			<bind nodeset="/data/LMD-VAL-meta_autoDate" calculate="now()" />
			<bind nodeset="/data/LMD-TIM-meta_dataEntry_startTime" type="time" jr:preload="timestamp" jr:preloadParams="start" />
			<bind nodeset="/data/LMD-TIM-meta_dataEntry_endTime" type="time" jr:preload="timestamp" jr:preloadParams="end" />
			<bind nodeset="/data/LMD-VAL-meta_dataSource" calculate="&quot;odk&quot;" />
			<bind nodeset="/data/LMD-VAL-meta_formVersion" calculate="6" />
			<bind nodeset="/data/LMD-VAL-meta_deviceID" calculate="property('deviceid')" />
			<bind nodeset="/data/LABEL-womanAge" />
			<bind nodeset="/data/LMD-VAL-manualDate" type="xsd:date" />
			<bind nodeset="/data/LMD-VAL-chwID" type="xsd:int" constraint=". &gt; 0" jr:constraintMsg="jr:itext('LMD-VAL-chwID-constraintMsg')" required="true()" />
			<bind nodeset="/data/LMD-VAL-communityID" type="xsd:int" constraint=". &gt; 0" jr:constraintMsg="jr:itext('LMD-VAL-communityID-constraintMsg')" required="true()" />
			<bind nodeset="/data/LMD-VAL-memberName" type="xsd:string" constraint="regex(., '^[a-zA-Z\s]+$')" jr:constraintMsg="jr:itext('LMD-VAL-memberName-constraintMsg')" required="true()" />
			<bind nodeset="/data/LMD-VAL-householdID" type="xsd:int" constraint=". &gt; 0" jr:constraintMsg="jr:itext('LMD-VAL-householdID-constraintMsg')" required="true()" />
			<bind nodeset="/data/GRP-ancVisitation" />
			<bind nodeset="/data/GRP-ancVisitation/LMD-VAL-birthInFiveYears" required="true()" />
			<bind nodeset="/data/GRP-ancVisitation/LMD-VAL-dateOfLastBirth" type="xsd:date" constraint=". &lt; /data/LMD-VAL-manualDate" jr:constraintMsg="jr:itext('GRP-ancVisitation/LMD-VAL-dateOfLastBirth-constraintMsg')" relevant="/data/GRP-ancVisitation/LMD-VAL-birthInFiveYears = '1'" required="true()" />
			<bind nodeset="/data/GRP-ancVisitation/LMD-VAL-clinicVisits" type="xsd:int" constraint=". &gt;= 0" relevant="/data/GRP-ancVisitation/LMD-VAL-birthInFiveYears = '1'" required="true()" />
			<bind nodeset="/data/GRP-ancVisitation/LMD-VAL-townVisits" type="xsd:int" constraint=". &gt;= 0" relevant="/data/GRP-ancVisitation/LMD-VAL-birthInFiveYears = '1'" required="true()" />
			<bind nodeset="/data/LMD-VAL-hasChildren" required="true()" />
			<bind nodeset="/data/LMD-VAL-numberOfChildren" type="xsd:int" constraint=". &gt; 0" jr:constraintMsg="jr:itext('LMD-VAL-numberOfChildren-constraintMsg')" relevant="/data/LMD-VAL-hasChildren = '1'" required="true()" />
			<bind nodeset="/data/LABEL-askQuestions" relevant="/data/LMD-VAL-hasChildren = '1'" />
			<bind nodeset="/data/LMD-RPT-staging_odk_children" relevant="/data/LMD-VAL-hasChildren = '1'" />
			<bind nodeset="/data/LMD-RPT-staging_odk_children/LMD-PULL" calculate="&quot;LMD-VAL-linkUUID, LMD-VAL-manualDate, LMD-VAL-chwID, LMD-VAL-communityID, LMD-VAL-memberID&quot;" />
			<bind nodeset="/data/LMD-RPT-staging_odk_children/LMD-VAL-meta_UUID" calculate="uuid()" />
			<bind nodeset="/data/LMD-RPT-staging_odk_children/LMD-VAL-childName" type="xsd:string" constraint="regex(., '^[a-zA-Z\s]+$') or . = 999" jr:constraintMsg="jr:itext('LMD-RPT-staging_odk_children/LMD-VAL-childName-constraintMsg')" required="true()" />
			<bind nodeset="/data/LMD-RPT-staging_odk_children/LMD-VAL-dateOfBirth" type="xsd:date" constraint=". &lt; /data/LMD-VAL-manualDate" jr:constraintMsg="jr:itext('LMD-RPT-staging_odk_children/LMD-VAL-dateOfBirth-constraintMsg')" required="true()" />
			<bind nodeset="/data/LMD-RPT-staging_odk_children/LMD-VAL-bornInClinic" required="true()" />
			<bind nodeset="/data/LMD-RPT-staging_odk_children/LMD-VAL-stillAlive" required="true()" />
			<bind nodeset="/data/LMD-RPT-staging_odk_children/GRP-ageAtDeath" relevant="/data/LMD-RPT-staging_odk_children/LMD-VAL-stillAlive = '0'" />
			<bind nodeset="/data/LMD-RPT-staging_odk_children/GRP-ageAtDeath/LMD-VAL-deathAgeYears" type="xsd:int" required="true()" />
			<bind nodeset="/data/LMD-RPT-staging_odk_children/GRP-ageAtDeath/LMD-VAL-deathAgeMonths" type="xsd:int" constraint=". &lt; 12" jr:constraintMsg="jr:itext('LMD-RPT-staging_odk_children/GRP-ageAtDeath/LMD-VAL-deathAgeMonths-constraintMsg')" required="true()" />
			<bind nodeset="/data/LMD-RPT-staging_odk_children/GRP-ageAtDeath/LMD-VAL-deathAgeDays" type="xsd:int" constraint=". &lt;= 31" jr:constraintMsg="jr:itext('LMD-RPT-staging_odk_children/GRP-ageAtDeath/LMD-VAL-deathAgeDays-constraintMsg')" required="true()" />
			<bind nodeset="/data/LMD-VAL-numberOfVaccineCards" type="xsd:int" constraint=". &gt;= 0" relevant="/data/LMD-VAL-hasChildren = '1'" required="true()" />
			<bind nodeset="/data/LMD-RPT-staging_odk_vaccine" />
			<bind nodeset="/data/LMD-RPT-staging_odk_vaccine/LMD-PULL" calculate="&quot;LMD-VAL-linkUUID, LMD-VAL-manualDate, LMD-VAL-chwID, LMD-VAL-communityID, LMD-VAL-memberID&quot;" />
			<bind nodeset="/data/LMD-RPT-staging_odk_vaccine/LMD-VAL-meta_UUID" calculate="uuid()" />
			<bind nodeset="/data/LMD-RPT-staging_odk_vaccine/LABEL-vaccineInfo" />
			<bind nodeset="/data/LMD-RPT-staging_odk_vaccine/LMD-VAL-childFirstName" type="xsd:string" constraint="regex(., '^[a-zA-Z\s]+$')" jr:constraintMsg="jr:itext('LMD-RPT-staging_odk_vaccine/LMD-VAL-childFirstName-constraintMsg')" required="true()" />
			<bind nodeset="/data/LMD-RPT-staging_odk_vaccine/LMD-CHK-vaccineReceived" required="true()" />
			<bind nodeset="/data/LMD-RPT-staging_odk_vaccine/LMD-VAL-BCG" type="xsd:date" constraint=". &lt; /data/LMD-VAL-manualDate" jr:constraintMsg="jr:itext('LMD-RPT-staging_odk_vaccine/LMD-VAL-BCG-constraintMsg')" relevant="selected(/data/LMD-RPT-staging_odk_vaccine/LMD-CHK-vaccineReceived, 'LMD-OPT-BCG')" required="true()" />
			<bind nodeset="/data/LMD-RPT-staging_odk_vaccine/LMD-VAL-OPV0" type="xsd:date" constraint=". &lt; /data/LMD-VAL-manualDate" jr:constraintMsg="jr:itext('LMD-RPT-staging_odk_vaccine/LMD-VAL-OPV0-constraintMsg')" relevant="selected(/data/LMD-RPT-staging_odk_vaccine/LMD-CHK-vaccineReceived, 'LMD-OPT-OPV0')" required="true()" />
			<bind nodeset="/data/LMD-RPT-staging_odk_vaccine/LMD-VAL-OPV1" type="xsd:date" constraint=". &lt; /data/LMD-VAL-manualDate" jr:constraintMsg="jr:itext('LMD-RPT-staging_odk_vaccine/LMD-VAL-OPV1-constraintMsg')" relevant="selected(/data/LMD-RPT-staging_odk_vaccine/LMD-CHK-vaccineReceived, 'LMD-OPT-OPV1')" required="true()" />
			<bind nodeset="/data/LMD-RPT-staging_odk_vaccine/LMD-VAL-OPV2" type="xsd:date" constraint=". &lt; /data/LMD-VAL-manualDate" jr:constraintMsg="jr:itext('LMD-RPT-staging_odk_vaccine/LMD-VAL-OPV2-constraintMsg')" relevant="selected(/data/LMD-RPT-staging_odk_vaccine/LMD-CHK-vaccineReceived, 'LMD-OPT-OPV2')" required="true()" />
			<bind nodeset="/data/LMD-RPT-staging_odk_vaccine/LMD-VAL-OPV3" type="xsd:date" constraint=". &lt; /data/LMD-VAL-manualDate" jr:constraintMsg="jr:itext('LMD-RPT-staging_odk_vaccine/LMD-VAL-OPV3-constraintMsg')" relevant="selected(/data/LMD-RPT-staging_odk_vaccine/LMD-CHK-vaccineReceived, 'LMD-OPT-OPV3')" required="true()" />
			<bind nodeset="/data/LMD-RPT-staging_odk_vaccine/LMD-VAL-Penta1" type="xsd:date" constraint=". &lt; /data/LMD-VAL-manualDate" jr:constraintMsg="jr:itext('LMD-RPT-staging_odk_vaccine/LMD-VAL-Penta1-constraintMsg')" relevant="selected(/data/LMD-RPT-staging_odk_vaccine/LMD-CHK-vaccineReceived, 'LMD-OPT-Penta1')" required="true()" />
			<bind nodeset="/data/LMD-RPT-staging_odk_vaccine/LMD-VAL-Penta2" type="xsd:date" constraint=". &lt; /data/LMD-VAL-manualDate" jr:constraintMsg="jr:itext('LMD-RPT-staging_odk_vaccine/LMD-VAL-Penta2-constraintMsg')" relevant="selected(/data/LMD-RPT-staging_odk_vaccine/LMD-CHK-vaccineReceived, 'LMD-OPT-Penta2')" required="true()" />
			<bind nodeset="/data/LMD-RPT-staging_odk_vaccine/LMD-VAL-Penta3" type="xsd:date" constraint=". &lt; /data/LMD-VAL-manualDate" jr:constraintMsg="jr:itext('LMD-RPT-staging_odk_vaccine/LMD-VAL-Penta3-constraintMsg')" relevant="selected(/data/LMD-RPT-staging_odk_vaccine/LMD-CHK-vaccineReceived, 'LMD-OPT-Penta3')" required="true()" />
			<bind nodeset="/data/LMD-RPT-staging_odk_vaccine/LMD-VAL-Rota1" type="xsd:date" constraint=". &lt; /data/LMD-VAL-manualDate" jr:constraintMsg="jr:itext('LMD-RPT-staging_odk_vaccine/LMD-VAL-Rota1-constraintMsg')" relevant="selected(/data/LMD-RPT-staging_odk_vaccine/LMD-CHK-vaccineReceived, 'LMD-OPT-Rota1')" required="true()" />
			<bind nodeset="/data/LMD-RPT-staging_odk_vaccine/LMD-VAL-Rota2" type="xsd:date" constraint=". &lt; /data/LMD-VAL-manualDate" jr:constraintMsg="jr:itext('LMD-RPT-staging_odk_vaccine/LMD-VAL-Rota2-constraintMsg')" relevant="selected(/data/LMD-RPT-staging_odk_vaccine/LMD-CHK-vaccineReceived, 'LMD-OPT-Rota2')" required="true()" />
			<bind nodeset="/data/LMD-RPT-staging_odk_vaccine/LMD-VAL-Rota3" type="xsd:date" constraint=". &lt; /data/LMD-VAL-manualDate" jr:constraintMsg="jr:itext('LMD-RPT-staging_odk_vaccine/LMD-VAL-Rota3-constraintMsg')" relevant="selected(/data/LMD-RPT-staging_odk_vaccine/LMD-CHK-vaccineReceived, 'LMD-OPT-Rota3')" required="true()" />
			<bind nodeset="/data/LMD-RPT-staging_odk_vaccine/LMD-VAL-Pneumo1" type="xsd:date" constraint=". &lt; /data/LMD-VAL-manualDate" jr:constraintMsg="jr:itext('LMD-RPT-staging_odk_vaccine/LMD-VAL-Pneumo1-constraintMsg')" relevant="selected(/data/LMD-RPT-staging_odk_vaccine/LMD-CHK-vaccineReceived, 'LMD-OPT-Pneuomo1')" required="true()" />
			<bind nodeset="/data/LMD-RPT-staging_odk_vaccine/LMD-VAL-Pneumo2" type="xsd:date" constraint=". &lt; /data/LMD-VAL-manualDate" jr:constraintMsg="jr:itext('LMD-RPT-staging_odk_vaccine/LMD-VAL-Pneumo2-constraintMsg')" relevant="selected(/data/LMD-RPT-staging_odk_vaccine/LMD-CHK-vaccineReceived, 'LMD-OPT-Pneumo2')" required="true()" />
			<bind nodeset="/data/LMD-RPT-staging_odk_vaccine/LMD-VAL-Pneumo3" type="xsd:date" constraint=". &lt; /data/LMD-VAL-manualDate" jr:constraintMsg="jr:itext('LMD-RPT-staging_odk_vaccine/LMD-VAL-Pneumo3-constraintMsg')" relevant="selected(/data/LMD-RPT-staging_odk_vaccine/LMD-CHK-vaccineReceived, 'LMD-OPT-Pneumo3')" required="true()" />
			<bind nodeset="/data/LMD-RPT-staging_odk_vaccine/LMD-VAL-IPV1" type="xsd:date" constraint=". &lt; /data/LMD-VAL-manualDate" jr:constraintMsg="jr:itext('LMD-RPT-staging_odk_vaccine/LMD-VAL-IPV1-constraintMsg')" relevant="selected(/data/LMD-RPT-staging_odk_vaccine/LMD-CHK-vaccineReceived, 'LMD-OPT-IPV1')" required="true()" />
			<bind nodeset="/data/LMD-RPT-staging_odk_vaccine/LMD-VAL-IPV2" type="xsd:date" constraint=". &lt; /data/LMD-VAL-manualDate" jr:constraintMsg="jr:itext('LMD-RPT-staging_odk_vaccine/LMD-VAL-IPV2-constraintMsg')" relevant="selected(/data/LMD-RPT-staging_odk_vaccine/LMD-CHK-vaccineReceived, 'LMD-OPT-IPV2')" required="true()" />
			<bind nodeset="/data/LMD-RPT-staging_odk_vaccine/LMD-VAL-IPV3" type="xsd:date" constraint=". &lt; /data/LMD-VAL-manualDate" jr:constraintMsg="jr:itext('LMD-RPT-staging_odk_vaccine/LMD-VAL-IPV3-constraintMsg')" relevant="selected(/data/LMD-RPT-staging_odk_vaccine/LMD-CHK-vaccineReceived, 'LMD-OPT-IPV3')" required="true()" />
			<bind nodeset="/data/LMD-RPT-staging_odk_vaccine/LMD-VAL-measles" type="xsd:date" constraint=". &lt; /data/LMD-VAL-manualDate" jr:constraintMsg="jr:itext('LMD-RPT-staging_odk_vaccine/LMD-VAL-measles-constraintMsg')" relevant="selected(/data/LMD-RPT-staging_odk_vaccine/LMD-CHK-vaccineReceived, 'LMD-OPT-measles')" required="true()" />
			<bind nodeset="/data/LMD-RPT-staging_odk_vaccine/LMD-VAL-yellowFever" type="xsd:date" constraint=". &lt; /data/LMD-VAL-manualDate" jr:constraintMsg="jr:itext('LMD-RPT-staging_odk_vaccine/LMD-VAL-yellowFever-constraintMsg')" relevant="selected(/data/LMD-RPT-staging_odk_vaccine/LMD-CHK-vaccineReceived, 'LMD-OPT-yellowFever')" required="true()" />
			<itext>
				<translation lang="en" default="">
					<text id="LABEL-womanAge-label">
						<value>Complete this form once per year for every woman between age 15-49</value>
					</text>
					<text id="LMD-VAL-manualDate-label">
						<value>Today's date:</value>
					</text>
					<text id="LMD-VAL-chwID-label">
						<value>CHW ID:</value>
					</text>
					<text id="LMD-VAL-chwID-constraintMsg">
						<value>CHW ID must be greater than 0</value>
					</text>
					<text id="LMD-VAL-communityID-label">
						<value>Community ID:</value>
					</text>
					<text id="LMD-VAL-communityID-constraintMsg">
						<value>Community ID must be greater than 0</value>
					</text>
					<text id="LMD-VAL-memberName-label">
						<value>Woman's name:</value>
					</text>
					<text id="LMD-VAL-memberName-constraintMsg">
						<value>Please only use letters to record the woman's name</value>
					</text>
					<text id="LMD-VAL-householdID-label">
						<value>Woman's household ID number:</value>
					</text>
					<text id="LMD-VAL-householdID-constraintMsg">
						<value>Household ID must be greater than 0</value>
					</text>
					<text id="GRP-ancVisitation-label">
						<value>A. ANC Visitation</value>
					</text>
					<text id="GRP-ancVisitation/LMD-VAL-birthInFiveYears-label">
						<value>In the past 5 years, you ever born a child?</value>
					</text>
					<text id="GRP-ancVisitation/LMD-VAL-birthInFiveYears-1-label">
						<value>Yes</value>
					</text>
					<text id="GRP-ancVisitation/LMD-VAL-birthInFiveYears-0-label">
						<value>No</value>
					</text>
					<text id="GRP-ancVisitation/LMD-VAL-dateOfLastBirth-label">
						<value>When you born your last child?</value>
					</text>
					<text id="GRP-ancVisitation/LMD-VAL-dateOfLastBirth-constraintMsg">
						<value>Must be before today</value>
					</text>
					<text id="GRP-ancVisitation/LMD-VAL-clinicVisits-label">
						<value>During your last belly, how many times you go to the clinic for a doctor or nurse to check on you?</value>
					</text>
					<text id="GRP-ancVisitation/LMD-VAL-townVisits-label">
						<value>During your last belly, how many times a doctor or nurse check on you in your own town here?</value>
					</text>
					<text id="LMD-VAL-hasChildren-label">
						<value>Does <output value="/data/LMD-VAL-memberName" /> have any children, either living or not living? </value>
					</text>
					<text id="LMD-VAL-hasChildren-1-label">
						<value>Yes</value>
					</text>
					<text id="LMD-VAL-hasChildren-0-label">
						<value>Does not have any children</value>
					</text>
					<text id="LMD-VAL-numberOfChildren-label">
						<value><output value="/data/LMD-VAL-memberName" />, how many children have you had (whether living or not living)?</value>
					</text>
					<text id="LMD-VAL-numberOfChildren-constraintMsg">
						<value>Woman has children. 0 cannot be entered. </value>
					</text>
					<text id="LABEL-askQuestions-label">
						<value>Ask the following questions about every child a woman has ever born, whether living or not living, starting with the first one</value>
					</text>
					<text id="LMD-RPT-staging_odk_children-label">
						<value>Children</value>
					</text>
					<text id="LMD-RPT-staging_odk_children/LMD-VAL-childName-label">
						<value>Enter the name of the child (can be living or not living)?</value>
					</text>
					<text id="LMD-RPT-staging_odk_children/LMD-VAL-childName-hint">
						<value>[Please enter 999 if child never got any name]</value>
					</text>
					<text id="LMD-RPT-staging_odk_children/LMD-VAL-childName-constraintMsg">
						<value>Please only use letters to record the child's name</value>
					</text>
					<text id="LMD-RPT-staging_odk_children/LMD-VAL-dateOfBirth-label">
						<value>Date of birth:</value>
					</text>
					<text id="LMD-RPT-staging_odk_children/LMD-VAL-dateOfBirth-constraintMsg">
						<value>Must be before today</value>
					</text>
					<text id="LMD-RPT-staging_odk_children/LMD-VAL-bornInClinic-label">
						<value>Was <output value="/data/LMD-RPT-staging_odk_children/LMD-VAL-childName" /> born in clinic?</value>
					</text>
					<text id="LMD-RPT-staging_odk_children/LMD-VAL-bornInClinic-1-label">
						<value>Yes</value>
					</text>
					<text id="LMD-RPT-staging_odk_children/LMD-VAL-bornInClinic-0-label">
						<value>No</value>
					</text>
					<text id="LMD-RPT-staging_odk_children/LMD-VAL-stillAlive-label">
						<value>Is <output value="/data/LMD-RPT-staging_odk_children/LMD-VAL-childName" /> still living?</value>
					</text>
					<text id="LMD-RPT-staging_odk_children/LMD-VAL-stillAlive-1-label">
						<value>Yes</value>
					</text>
					<text id="LMD-RPT-staging_odk_children/LMD-VAL-stillAlive-0-label">
						<value>No</value>
					</text>
					<text id="LMD-RPT-staging_odk_children/GRP-ageAtDeath-label">
						<value>Age at Death</value>
					</text>
					<text id="LMD-RPT-staging_odk_children/GRP-ageAtDeath/LMD-VAL-deathAgeYears-label">
						<value>How old was the child when he/she passed away?

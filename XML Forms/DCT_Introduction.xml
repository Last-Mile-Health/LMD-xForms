<?xml version="1.0" encoding="UTF-8" ?>
<h:html xmlns:h="http://www.w3.org/1999/xhtml" xmlns:orx="http://openrosa.org/jr/xforms" xmlns="http://www.w3.org/2002/xforms" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:jr="http://openrosa.org/javarosa" xmlns:vellum="http://commcarehq.org/xforms/vellum">
	<h:head>
		<h:title>DCT Introduction</h:title>
		<model>
			<instance>
				<data xmlns:jrm="http://dev.commcarehq.org/jr/xforms" xmlns="http://openrosa.org/formdesigner/CC2BDB62-D216-48CD-92AF-960108422508" uiVersion="2" version="2" name="DCT Introduction">
					<LMD-DATABASE />
					<LMD-TABLE />
					<LMD-VAL-meta_UUID />
					<LMD-VAL-meta_autoDate />
					<LMD-TIM-meta_dataEntry_startTime />
					<LMD-TIM-meta_dataEntry_endTime />
					<LMD-VAL-meta_dataSource />
					<LMD-VAL-meta_formVersion />
					<LMD-VAL-meta_deviceID />
					<LABEL-introductionSwipe />
					<LMD-VAL-numberPracticeOne />
					<LMD-VAL-numberPracticeTwo />
					<LMD-VAL-text />
					<LMD-VAL-singleSelect />
					<LABEL-swipeBack />
					<LABEL-swipeForwardDoorTwo />
					<LABEL-swipeForwardDoorThree />
					<LMD-CHK-multiSelect />
					<GRP-MultipleQuestions>
						<LMD-VAL-numberEars />
						<LMD-VAL-numberFingers />
						<LMD-VAL-numberArms />
					</GRP-MultipleQuestions>
					<LMD-VAL-dateBeforeToday />
					<LMD-VAL-birthMonthAndYear />
					<LMD-TIM-time />
					<LABEL-complete />
				</data>
			</instance>
			<bind nodeset="/data/LMD-DATABASE" calculate="&quot;lastmile_chwdb&quot;" />
			<bind nodeset="/data/LMD-TABLE" calculate="&quot;staging_odk_DCTIntroduction&quot;" />
			<bind nodeset="/data/LMD-VAL-meta_UUID" calculate="uuid()" />
			<bind nodeset="/data/LMD-VAL-meta_autoDate" calculate="now()" />
			<bind nodeset="/data/LMD-TIM-meta_dataEntry_startTime" type="time" jr:preload="timestamp" jr:preloadParams="start" />
			<bind nodeset="/data/LMD-TIM-meta_dataEntry_endTime" type="time" jr:preload="timestamp" jr:preloadParams="end" />
			<bind nodeset="/data/LMD-VAL-meta_dataSource" calculate="&quot;odk&quot;" />
			<bind nodeset="/data/LMD-VAL-meta_formVersion" calculate="2" />
			<bind nodeset="/data/LMD-VAL-meta_deviceID" calculate="property('deviceid')" />
			<bind nodeset="/data/LABEL-introductionSwipe" />
			<bind nodeset="/data/LMD-VAL-numberPracticeOne" type="xsd:int" required="true()" />
			<bind nodeset="/data/LMD-VAL-numberPracticeTwo" type="xsd:int" constraint=". &gt; 10" jr:constraintMsg="jr:itext('LMD-VAL-numberPracticeTwo-constraintMsg')" required="true()" />
			<bind nodeset="/data/LMD-VAL-text" type="xsd:string" required="true()" />
			<bind nodeset="/data/LMD-VAL-singleSelect" required="true()" />
			<bind nodeset="/data/LABEL-swipeBack" type="xsd:int" constraint=". = 97854248" relevant="/data/LMD-VAL-singleSelect = 'Door1'" required="true()" />
			<bind nodeset="/data/LABEL-swipeForwardDoorTwo" relevant="/data/LMD-VAL-singleSelect = 'Door2'" />
			<bind nodeset="/data/LABEL-swipeForwardDoorThree" relevant="/data/LMD-VAL-singleSelect = 'Door3'" />
			<bind nodeset="/data/LMD-CHK-multiSelect" constraint="selected(., 'E') and selected(., 'B') and selected(., 'A') and selected(., 'H') and selected(., &quot;N&quot;) and not(selected(., &quot;C&quot;)) and not(selected(., &quot;D&quot;)) and not(selected(., &quot;F&quot;)) and not(selected(., &quot;G&quot;)) and not(selected(., &quot;I&quot;)) and not(selected(., &quot;J&quot;)) and not(selected(., &quot;K&quot;)) and not(selected(., &quot;L&quot;)) and not(selected(., &quot;M&quot;)) and not(selected(., &quot;O&quot;))" jr:constraintMsg="jr:itext('LMD-CHK-multiSelect-constraintMsg')" required="true()" />
			<bind nodeset="/data/GRP-MultipleQuestions" />
			<bind nodeset="/data/GRP-MultipleQuestions/LMD-VAL-numberEars" type="xsd:int" required="true()" />
			<bind nodeset="/data/GRP-MultipleQuestions/LMD-VAL-numberFingers" type="xsd:int" required="true()" />
			<bind nodeset="/data/GRP-MultipleQuestions/LMD-VAL-numberArms" type="xsd:int" required="true()" />
			<bind nodeset="/data/LMD-VAL-dateBeforeToday" type="xsd:date" constraint=". &lt; today()" jr:constraintMsg="jr:itext('LMD-VAL-dateBeforeToday-constraintMsg')" required="true()" />
			<bind nodeset="/data/LMD-VAL-birthMonthAndYear" type="xsd:date" required="true()" />
			<bind nodeset="/data/LMD-TIM-time" type="xsd:time" required="true()" />
			<bind nodeset="/data/LABEL-complete" />
			<itext>
				<translation lang="en" default="">
					<text id="LABEL-introductionSwipe-label">
						<value>Practice SWIPING. Go to the next screen</value>
					</text>
					<text id="LMD-VAL-numberPracticeOne-label">
						<value>Enter a number:</value>
					</text>
					<text id="LMD-VAL-numberPracticeTwo-label">
						<value>Enter another number:</value>
					</text>
					<text id="LMD-VAL-numberPracticeTwo-constraintMsg">
						<value>The number must be more than 10!</value>
					</text>
					<text id="LMD-VAL-text-label">
						<value>Enter your name:</value>
					</text>
					<text id="LMD-VAL-singleSelect-label">
						<value>Which door should we open?</value>
					</text>
					<text id="LMD-VAL-singleSelect-Door1-label">
						<value>Door 1</value>
					</text>
					<text id="LMD-VAL-singleSelect-Door2-label">
						<value>Door 2</value>
					</text>
					<text id="LMD-VAL-singleSelect-Door3-label">
						<value>Door 3</value>
					</text>
					<text id="LABEL-swipeBack-label">
						<value>You opened Door 1! Go back and open door 2 or door 3!</value>
					</text>
					<text id="LABEL-swipeForwardDoorTwo-label">
						<value>You opened door 2! See how this program automatically tells you the right answer?</value>
					</text>
					<text id="LABEL-swipeForwardDoorThree-label">
						<value>You opened door 3! See how this program automatically tells you the right answer?</value>
					</text>
					<text id="LMD-CHK-multiSelect-label">
						<value>[Multiple] Chose options A, B, E, H, and N</value>
					</text>
					<text id="LMD-CHK-multiSelect-constraintMsg">
						<value>Look closely at the instructions...</value>
					</text>
					<text id="LMD-CHK-multiSelect-LMD-OPT-A-label">
						<value>A</value>
					</text>
					<text id="LMD-CHK-multiSelect-LMD-OPT-B-label">
						<value>B</value>
					</text>
					<text id="LMD-CHK-multiSelect-LMD-OPT-C-label">
						<value>C</value>
					</text>
					<text id="LMD-CHK-multiSelect-LMD-OPT-D-label">
						<value>D</value>
					</text>
					<text id="LMD-CHK-multiSelect-LMD-OPT-E-label">
						<value>E</value>
					</text>
					<text id="LMD-CHK-multiSelect-LMD-OPT-F-label">
						<value>F</value>
					</text>
					<text id="LMD-CHK-multiSelect-LMD-OPT-G-label">
						<value>G</value>
					</text>
					<text id="LMD-CHK-multiSelect-LMD-OPT-H-label">
						<value>H</value>
					</text>
					<text id="LMD-CHK-multiSelect-LMD-OPT-I-label">
						<value>I</value>
					</text>
					<text id="LMD-CHK-multiSelect-LMD-OPT-J-label">
						<value>J</value>
					</text>
					<text id="LMD-CHK-multiSelect-LMD-OPT-K-label">
						<value>K</value>
					</text>
					<text id="LMD-CHK-multiSelect-LMD-OPT-L-label">
						<value>L</value>
					</text>
					<text id="LMD-CHK-multiSelect-LMD-OPT-M-label">
						<value>M</value>
					</text>
					<text id="LMD-CHK-multiSelect-LMD-OPT-N-label">
						<value>N</value>
					</text>
					<text id="LMD-CHK-multiSelect-LMD-OPT-O-label">
						<value>O</value>
					</text>
					<text id="GRP-MultipleQuestions-label">
						<value>GRP-MultipleQuestions</value>
					</text>
					<text id="GRP-MultipleQuestions/LMD-VAL-numberEars-label">
						<value>How many ears do you have?</value>
					</text>
					<text id="GRP-MultipleQuestions/LMD-VAL-numberFingers-label">
						<value>How many fingers do you have?</value>
					</text>
					<text id="GRP-MultipleQuestions/LMD-VAL-numberArms-label">
						<value>How many arms do you have?</value>
					</text>
					<text id="LMD-VAL-dateBeforeToday-label">
						<value>Enter a date before today:</value>
					</text>
					<text id="LMD-VAL-dateBeforeToday-constraintMsg">
						<value>Date must be before today</value>
					</text>
					<text id="LMD-VAL-birthMonthAndYear-label">
						<value>Enter your birth month and year in the calendar:</value>
					</text>
					<text id="LMD-TIM-time-label">
						<value>Change time to 10:15 am</value>
					</text>
					<text id="LABEL-complete-label">
						<value>Great work <output value="/data/LMD-VAL-text" />! You are now ready to learn more forms! Save this form and exit</value>
					</text>
				</translation>
			</itext>
		</model>
	</h:head>
	<h:body>
		<trigger ref="/data/LABEL-introductionSwipe" appearance="minimal">
			<label ref="jr:itext('LABEL-introductionSwipe-label')" />
		</trigger>
		<input ref="/data/LMD-VAL-numberPracticeOne">
			<label ref="jr:itext('LMD-VAL-numberPracticeOne-label')" />
		</input>
		<input ref="/data/LMD-VAL-numberPracticeTwo">
			<label ref="jr:itext('LMD-VAL-numberPracticeTwo-label')" />
			<alert ref="jr:itext('LMD-VAL-numberPracticeTwo-constraintMsg')" />
		</input>
		<input ref="/data/LMD-VAL-text">
			<label ref="jr:itext('LMD-VAL-text-label')" />
		</input>
		<select1 ref="/data/LMD-VAL-singleSelect">
			<label ref="jr:itext('LMD-VAL-singleSelect-label')" />
			<item>
				<label ref="jr:itext('LMD-VAL-singleSelect-Door1-label')" />
				<value>Door1</value>
			</item>
			<item>
				<label ref="jr:itext('LMD-VAL-singleSelect-Door2-label')" />
				<value>Door2</value>
			</item>
			<item>
				<label ref="jr:itext('LMD-VAL-singleSelect-Door3-label')" />
				<value>Door3</value>
			</item>
		</select1>
		<input ref="/data/LABEL-swipeBack">
			<label ref="jr:itext('LABEL-swipeBack-label')" />
		</input>
		<trigger ref="/data/LABEL-swipeForwardDoorTwo" appearance="minimal">
			<label ref="jr:itext('LABEL-swipeForwardDoorTwo-label')" />
		</trigger>
		<trigger ref="/data/LABEL-swipeForwardDoorThree" appearance="minimal">
			<label ref="jr:itext('LABEL-swipeForwardDoorThree-label')" />
		</trigger>
		<select ref="/data/LMD-CHK-multiSelect">
			<label ref="jr:itext('LMD-CHK-multiSelect-label')" />
			<alert ref="jr:itext('LMD-CHK-multiSelect-constraintMsg')" />
			<item>
				<label ref="jr:itext('LMD-CHK-multiSelect-LMD-OPT-A-label')" />
				<value>LMD-OPT-A</value>
			</item>
			<item>
				<label ref="jr:itext('LMD-CHK-multiSelect-LMD-OPT-B-label')" />
				<value>LMD-OPT-B</value>
			</item>
			<item>
				<label ref="jr:itext('LMD-CHK-multiSelect-LMD-OPT-C-label')" />
				<value>LMD-OPT-C</value>
			</item>
			<item>
				<label ref="jr:itext('LMD-CHK-multiSelect-LMD-OPT-D-label')" />
				<value>LMD-OPT-D</value>
			</item>
			<item>
				<label ref="jr:itext('LMD-CHK-multiSelect-LMD-OPT-E-label')" />
				<value>LMD-OPT-E</value>
			</item>
			<item>
				<label ref="jr:itext('LMD-CHK-multiSelect-LMD-OPT-F-label')" />
				<value>LMD-OPT-F</value>
			</item>
			<item>
				<label ref="jr:itext('LMD-CHK-multiSelect-LMD-OPT-G-label')" />
				<value>LMD-OPT-G</value>
			</item>
			<item>
				<label ref="jr:itext('LMD-CHK-multiSelect-LMD-OPT-H-label')" />
				<value>LMD-OPT-H</value>
			</item>
			<item>
				<label ref="jr:itext('LMD-CHK-multiSelect-LMD-OPT-I-label')" />
				<value>LMD-OPT-I</value>
			</item>
			<item>
				<label ref="jr:itext('LMD-CHK-multiSelect-LMD-OPT-J-label')" />
				<value>LMD-OPT-J</value>
			</item>
			<item>
				<label ref="jr:itext('LMD-CHK-multiSelect-LMD-OPT-K-label')" />
				<value>LMD-OPT-K</value>
			</item>
			<item>
				<label ref="jr:itext('LMD-CHK-multiSelect-LMD-OPT-L-label')" />
				<value>LMD-OPT-L</value>
			</item>
			<item>
				<label ref="jr:itext('LMD-CHK-multiSelect-LMD-OPT-M-label')" />
				<value>LMD-OPT-M</value>
			</item>
			<item>
				<label ref="jr:itext('LMD-CHK-multiSelect-LMD-OPT-N-label')" />
				<value>LMD-OPT-N</value>
			</item>
			<item>
				<label ref="jr:itext('LMD-CHK-multiSelect-LMD-OPT-O-label')" />
				<value>LMD-OPT-O</value>
			</item>
		</select>
		<group ref="/data/GRP-MultipleQuestions" appearance="field-list">
			<label ref="jr:itext('GRP-MultipleQuestions-label')" />
			<input ref="/data/GRP-MultipleQuestions/LMD-VAL-numberEars">
				<label ref="jr:itext('GRP-MultipleQuestions/LMD-VAL-numberEars-label')" />
			</input>
			<input ref="/data/GRP-MultipleQuestions/LMD-VAL-numberFingers">
				<label ref="jr:itext('GRP-MultipleQuestions/LMD-VAL-numberFingers-label')" />
			</input>
			<input ref="/data/GRP-MultipleQuestions/LMD-VAL-numberArms">
				<label ref="jr:itext('GRP-MultipleQuestions/LMD-VAL-numberArms-label')" />
			</input>
		</group>
		<input ref="/data/LMD-VAL-dateBeforeToday" appearance="no-calendar">
			<label ref="jr:itext('LMD-VAL-dateBeforeToday-label')" />
			<alert ref="jr:itext('LMD-VAL-dateBeforeToday-constraintMsg')" />
		</input>
		<input ref="/data/LMD-VAL-birthMonthAndYear" appearance="month-year">
			<label ref="jr:itext('LMD-VAL-birthMonthAndYear-label')" />
		</input>
		<input ref="/data/LMD-TIM-time">
			<label ref="jr:itext('LMD-TIM-time-label')" />
		</input>
		<trigger ref="/data/LABEL-complete" appearance="minimal">
			<label ref="jr:itext('LABEL-complete-label')" />
		</trigger>
	</h:body>
</h:html>
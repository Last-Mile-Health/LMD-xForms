	<h:body>
		<input ref="/data/LMD-VAL-ccsID">
			<label ref="jr:itext('LMD-VAL-ccsID-label')" />
			<alert ref="jr:itext('LMD-VAL-ccsID-constraintMsg')" />
		</input>
		<input ref="/data/LMD-VAL-manualDate" appearance="no-calendar">
			<label ref="jr:itext('LMD-VAL-manualDate-label')" />
		</input>
		<input ref="/data/LMD-VAL-childName">
			<label ref="jr:itext('LMD-VAL-childName-label')" />
			<alert ref="jr:itext('LMD-VAL-childName-constraintMsg')" />
		</input>
		<input ref="/data/LMD-VAL-childDOB" appearance="no-calendar">
			<label ref="jr:itext('LMD-VAL-childDOB-label')" />
			<alert ref="jr:itext('LMD-VAL-childDOB-constraintMsg')" />
		</input>
		<input ref="/data/LMD-VAL-childHHID">
			<label ref="jr:itext('LMD-VAL-childHHID-label')" />
			<alert ref="jr:itext('LMD-VAL-childHHID-constraintMsg')" />
		</input>
		<select1 ref="/data/LMD-VAL-childGender">
			<label ref="jr:itext('LMD-VAL-childGender-label')" />
			<item>
				<label ref="jr:itext('LMD-VAL-childGender-0-label')" />
				<value>0</value>
			</item>
			<item>
				<label ref="jr:itext('LMD-VAL-childGender-1-label')" />
				<value>1</value>
			</item>
		</select1>
		<input ref="/data/LMD-VAL-chwID">
			<label ref="jr:itext('LMD-VAL-chwID-label')" />
			<alert ref="jr:itext('LMD-VAL-chwID-constraintMsg')" />
		</input>
		<input ref="/data/LMD-VAL-communityID">
			<label ref="jr:itext('LMD-VAL-communityID-label')" />
			<alert ref="jr:itext('LMD-VAL-communityID-constraintMsg')" />
		</input>
		<select ref="/data/LMD-CHK-vaccineBridge">
			<label ref="jr:itext('LMD-CHK-vaccineBridge-label')" />
			<item>
				<label ref="jr:itext('LMD-CHK-vaccineBridge-LMD-OPT-bcg-label')" />
				<value>LMD-OPT-bcg</value>
			</item>
			<item>
				<label ref="jr:itext('LMD-CHK-vaccineBridge-LMD-OPT-opv0-label')" />

					<LMD-DATABASE />
					<LMD-TABLE />
					<LMD-VAL-meta_UUID />
					<LMD-VAL-meta_autoDate />
					<LMD-TIM-meta_dataEntry_startTime />
					<LMD-TIM-meta_dataEntry_endTime />
					<LMD-VAL-meta_dataSource />
					<LMD-VAL-meta_formVersion />
					<LMD-VAL-meta_deviceID />
					<LMD-VAL-ccsID />
					<LMD-VAL-manualDate />
					<LMD-VAL-childName />
					<LMD-VAL-childDOB />
					<LMD-VAL-childHHID />
					<LMD-VAL-childGender />
					<LMD-VAL-chwID />
					<LMD-VAL-communityID />
					<LMD-CHK-vaccineBridge />
					<GRP-Round0Vaccines>
						<LMD-VAL-bcgLocation />
						<LMD-VAL-opv0Location />
					</GRP-Round0Vaccines>
					<GRP-Round1Vaccines>
						<LMD-VAL-opv1Location />
						<LMD-VAL-rota1Location />
						<LMD-VAL-penta1Location />
						<LMD-VAL-pneumo1Location />
					</GRP-Round1Vaccines>
					<GRP-Round2Vaccines>
						<LMD-VAL-opv2Location />
						<LMD-VAL-rota2Location />
						<LMD-VAL-penta2Location />
						<LMD-VAL-pneumo2Location />
					</GRP-Round2Vaccines>
					<GRP-Round3Vaccines>
						<LMD-VAL-opv3Location />
						<LMD-VAL-rota3Location />
						<LMD-VAL-penta3Location />
						<LMD-VAL-pneumo3Location />
					</GRP-Round3Vaccines>
					<GRP-Round4Vaccines>
						<LMD-VAL-yellowfeverLocation />
						<LMD-VAL-measlesLocation />
					</GRP-Round4Vaccines>
				</data>
			</instance>
			<bind nodeset="/data/LMD-DATABASE" calculate="&quot;lastmile_chwdb&quot;" />
			<bind nodeset="/data/LMD-TABLE" calculate="&quot;staging_odk_vaccinetracker&quot;" />
			<bind nodeset="/data/LMD-VAL-meta_UUID" calculate="uuid()" />
			<bind nodeset="/data/LMD-VAL-meta_autoDate" calculate="now()" />
			<bind nodeset="/data/LMD-TIM-meta_dataEntry_startTime" type="time" jr:preload="timestamp" jr:preloadParams="start" />
			<bind nodeset="/data/LMD-TIM-meta_dataEntry_endTime" type="time" jr:preload="timestamp" jr:preloadParams="end" />
			<bind nodeset="/data/LMD-VAL-meta_dataSource" calculate="&quot;odk&quot;" />
			<bind nodeset="/data/LMD-VAL-meta_formVersion" calculate="2" />
			<bind nodeset="/data/LMD-VAL-meta_deviceID" calculate="property('deviceid')" />
			<bind nodeset="/data/LMD-VAL-ccsID" type="xsd:int" constraint=". &gt; 0" jr:constraintMsg="jr:itext('LMD-VAL-ccsID-constraintMsg')" required="true()" />
			<bind nodeset="/data/LMD-VAL-manualDate" type="xsd:date" required="true()" />
			<bind nodeset="/data/LMD-VAL-childName" type="xsd:string" constraint="regex(., '^[a-zA-Z\s]+$')" jr:constraintMsg="jr:itext('LMD-VAL-childName-constraintMsg')" required="true()" />
			<bind nodeset="/data/LMD-VAL-childDOB" type="xsd:date" constraint=". &lt; today()" jr:constraintMsg="jr:itext('LMD-VAL-childDOB-constraintMsg')" required="true()" />
			<bind nodeset="/data/LMD-VAL-childHHID" type="xsd:int" constraint=". &gt; 0" jr:constraintMsg="jr:itext('LMD-VAL-childHHID-constraintMsg')" required="true()" />
			<bind nodeset="/data/LMD-VAL-childGender" required="true()" />
			<bind nodeset="/data/LMD-VAL-chwID" type="xsd:int" constraint=". &gt; 0" jr:constraintMsg="jr:itext('LMD-VAL-chwID-constraintMsg')" required="true()" />
			<bind nodeset="/data/LMD-VAL-communityID" type="xsd:int" constraint=". &gt; 0" jr:constraintMsg="jr:itext('LMD-VAL-communityID-constraintMsg')" required="true()" />
			<bind nodeset="/data/LMD-CHK-vaccineBridge" required="true()" />
			<bind nodeset="/data/GRP-Round0Vaccines" />
			<bind nodeset="/data/GRP-Round0Vaccines/LMD-VAL-bcgLocation" relevant="selected(/data/LMD-CHK-vaccineBridge, 'LMD-OPT-bcg')" required="true()" />
			<bind nodeset="/data/GRP-Round0Vaccines/LMD-VAL-opv0Location" relevant="selected(/data/LMD-CHK-vaccineBridge, 'LMD-OPT-opv0')" required="true()" />
			<bind nodeset="/data/GRP-Round1Vaccines" />
			<bind nodeset="/data/GRP-Round1Vaccines/LMD-VAL-opv1Location" relevant="selected(/data/LMD-CHK-vaccineBridge, 'LMD-OPT-opv1')" required="true()" />
			<bind nodeset="/data/GRP-Round1Vaccines/LMD-VAL-rota1Location" relevant="selected(/data/LMD-CHK-vaccineBridge, 'LMD-OPT-rota1')" required="true()" />
			<bind nodeset="/data/GRP-Round1Vaccines/LMD-VAL-penta1Location" relevant="selected(/data/LMD-CHK-vaccineBridge, 'LMD-OPT-penta1')" required="true()" />
			<bind nodeset="/data/GRP-Round1Vaccines/LMD-VAL-pneumo1Location" relevant="selected(/data/LMD-CHK-vaccineBridge, 'LMD-OPT-pneumo1')" required="true()" />
